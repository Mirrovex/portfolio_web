{
    "version": 2,
    "builds": [
        {
            "src": "Dockerfile",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "portfolio_web/wsgi.py"
        }
    ]
}